{
  "sessions_file": "sessions_latest.csv",
  "workbook_file": "data/910CPR_Lander_Workbook_Final.xlsx",
  "template_file": "scripts/session_template.html",
  "site_base": "https://www.910cpr.com",
  "output_dir": "sessions",
  "mode": "daily",
  "age_days": 365,
  "emit_sitemaps": true,

  "columns": {
    "id":     ["Session ID","ID","HOVN_ID","ClassId","Class ID"],
    "family": ["course_family","Course Family","Family","Program","Program Code"],
    "title":  ["title","Course Title","Class Title","Name"],
    "start":  ["start","Start","Start Date","Start Date/Time","Start Time","DateTime Start"],
    "end":    ["end","End","End Date","End Date/Time","End Time","DateTime End"],
    "city":   ["city","City","Location City","Town"],
    "state":  ["state","State","ST"],
    "zip":    ["zip","Postal Code","ZIP","Zip Code"],
    "venue":  ["venue","Location Name","Location","Site","Room"],
    "address":["address","Street","Address","Address 1","Street Address"],
    "url":    ["url","Buy URL","Registration Link","Public URL","HOVN URL"]
  },

  "workbook_sheets": {
    "descriptions": ["Descriptions","Course Descriptions","Sheet1","Courses"],
    "aliases": ["Aliases","Map","Families","Lookups"]
  }
}
