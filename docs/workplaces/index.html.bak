  <script>
  (()=>{
    if (window.__schedLoaded_work) return; window.__schedLoaded_work = true;
    const FEED=["/periscope_full.json","../periscope_full.json","../../periscope_full.json","./periscope_full.json"];
    async function getFeed(){ for(const u of FEED){ try{const r=await fetch(u,{cache:"no-store"}); if(r.ok) return r.json(); }catch{} } return []; }
    const U=s=>(s||"").toUpperCase(); const textOf=o=>Object.values(o).filter(v=>typeof v==="string").join(" ");
    const LIM=15;
    function norm(it){ const dt=new Date(it.start||it.date||""); const ok=(dt instanceof Date)&&!isNaN(dt);
      return {ok,dt,city:it.city||"",url:it.url||it.link||"#",text:U(textOf(it))}; }
    const has=(n,re)=>re.test(n.text);
    const isHeartsaver = n => has(n,/\bHEARTSAVER\b/);
    const isHSI = n => has(n,/(^|[^A-Z])HSI([^A-Z]|$)|HEALTH\s*&\s*SAFETY\s*INSTITUTE/);
    const isBlended = n => has(n,/\bBLENDED\b|ONLINE\s*\+\s*SKILLS|SKILLS\s*SESSION/);
    const isInPerson = n => has(n,/IN\s*PERSON|CLASSROOM|INSTRUCTOR[-\s]?LED/) && !isBlended(n);

    function label(n){ const d=n.dt; const mo=d.toLocaleString(undefined,{month:"short"}); const day=String(d.getDate()).padStart(2,"0");
      let h=d.getHours(),m=d.getMinutes(),ap=h>=12?"p":"a"; h=h%12||12; const hm=m?`${h}:${String(m).padStart(2,"0")}${ap}`:`${h}${ap}`;
      return `${mo} ${day}, ${hm}${n.city?` — ${n.city}`:""}`;}
    function setChips(id,items){ const el=document.getElementById(id); if(!el) return;
      const html=items.map(n=>`<a class="timechip" href="${n.url}">${label(n)}</a>`).join("");
      el.innerHTML = html || '<span class="micro">New dates posting—check back soon.</span>'; }

    (async()=>{
      const now=new Date();
      const items=(await getFeed()).map(norm).filter(x=>x.ok && x.dt>=now).sort((a,b)=>a.dt-b.dt);

      setChips("hs-inperson", items.filter(n => isHeartsaver(n)&&isInPerson(n)).slice(0,LIM));
      setChips("hs-blended",  items.filter(n => isHeartsaver(n)&&isBlended(n)).slice(0,LIM));
      setChips("hsi-blended", items.filter(n => isHSI(n)&&isBlended(n)).slice(0,LIM));
    })();
  })();
  </script>
