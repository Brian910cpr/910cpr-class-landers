  <script>
  (()=>{
    if (window.__schedLoaded_med) return; window.__schedLoaded_med = true;
    const FEED=["/periscope_full.json","../periscope_full.json","../../periscope_full.json","./periscope_full.json"];

    async function getFeed(){
      for(const u of FEED){ try{ const r=await fetch(u,{cache:"no-store"}); if(r.ok){return r.json();} }catch{} }
      return [];
    }
    const U=s=>(s||"").toUpperCase();
    const textOf=o=>Object.values(o).filter(v=>typeof v==="string").join(" ");
    const LIM=15;

    function norm(it){
      const dt = new Date(it.start||it.date||"");
      const ok = (dt instanceof Date)&&!isNaN(dt);
      const city = it.city||it.City||"";
      const url = it.url||it.link||"#";
      return {ok,dt,city,url,text:U(textOf(it))};
    }
    const has=(n,re)=>re.test(n.text);
    const family = n =>
      has(n,/([^A-Z]|^)BLS([^A-Z]|$)|BASIC\s+LIFE\s+SUPPORT/)? "BLS" :
      has(n,/([^A-Z]|^)ACLS([^A-Z]|$)|ADVANCED\s+CARDIO/)? "ACLS" :
      has(n,/([^A-Z]|^)PALS([^A-Z]|$)|PEDIATRIC\s+ADVANCED/)? "PALS" : null;
    const isHeart = n => has(n,/HEART\s*CODE|HEARTCODE|ONLINE.*SKILLS|SKILLS\s*ASSESSMENT/);
    const isRenew = n => has(n,/\bRENEW(AL)?\b|RECERT|UPDATE\b/);
    const isInPer = n => has(n,/IN\s*PERSON|CLASSROOM|INSTRUCTOR[-\s]?LED/) && !isHeart(n);

    function label(n){
      const d=n.dt; const mo=d.toLocaleString(undefined,{month:"short"}); const day=String(d.getDate()).padStart(2,"0");
      let h=d.getHours(),m=d.getMinutes(),ap=h>=12?"p":"a"; h=h%12||12;
      const hm = m?`${h}:${String(m).padStart(2,"0")}${ap}`:`${h}${ap}`;
      return `${mo} ${day}, ${hm}${n.city?` — ${n.city}`:""}`;
    }
    function setChips(id,items){
      const el=document.getElementById(id); if(!el) return;
      const html=items.map(n=>`<a class="timechip" href="${n.url}">${label(n)}</a>`).join("");
      el.innerHTML = html || '<span class="micro">New dates posting—check back soon.</span>';
    }

    function tri(fam){ return {
      ren: n => family(n)===fam && isRenew(n)  && !isHeart(n),
      init:n => family(n)===fam && !isRenew(n) && isInPer(n),
      heart:n => family(n)===fam && isHeart(n)
    };}

    (async()=>{
      const raw=await getFeed(); const now=new Date();
      const items=raw.map(norm).filter(x=>x.ok && x.dt>=now).sort((a,b)=>a.dt-b.dt);
      const BLS=tri("BLS"), ACLS=tri("ACLS"), PALS=tri("PALS");

      setChips("bls-ren", items.filter(BLS.ren).slice(0,LIM));
      setChips("bls-init",items.filter(BLS.init).slice(0,LIM));
      setChips("bls-heart",items.filter(BLS.heart).slice(0,LIM));

      setChips("acls-ren", items.filter(ACLS.ren).slice(0,LIM));
      setChips("acls-init",items.filter(ACLS.init).slice(0,LIM));
      setChips("acls-heart",items.filter(ACLS.heart).slice(0,LIM));

      setChips("pals-ren", items.filter(PALS.ren).slice(0,LIM));
      setChips("pals-init",items.filter(PALS.init).slice(0,LIM));
      setChips("pals-heart",items.filter(PALS.heart).slice(0,LIM));
    })();
  })();
  </script>
